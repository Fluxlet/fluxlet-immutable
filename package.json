{
  "name": "fluxlet-immutable",
  "version": "0.0.1",
  "description": "Immutable state utilities for Fluxlet",
  "main": "lib/index",
  "registry": "jspm",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.23",
    "jspm": "^0.16.12",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "mocha": "^2.3.3"
  },
  "scripts": {
    "prepublish": "babel -d lib src",
    "pretest": "jspm install -y",
    "test": "karma start --single-run"
  },
  "jspm": {
    "main": "index",
    "format": "es6",
    "directories": {
      "lib": "src"
    },
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "chai": "npm:chai@^3.3.0",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "repository": "fluxlet/fluxlet-immutable",
  "keywords": [
    "fluxlet",
    "immutable"
  ],
  "author": "Mark Gibson <mgibson@adaptavist.com>",
  "license": "MIT"
}
